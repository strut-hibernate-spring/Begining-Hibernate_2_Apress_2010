
    drop table Publisher if exists;

    drop table Subscriber if exists;

    create table Publisher (
        id integer generated by default as identity (start with 1),
        username varchar(255),
        primary key (id),
        unique (username)
    );

    create table Subscriber (
        id integer generated by default as identity (start with 1),
        username varchar(255),
        primary key (id),
        unique (username)
    );
